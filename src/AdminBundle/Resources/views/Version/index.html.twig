{% extends "@Admin/base_home.html.twig" %}

{% block content %}
    <div class="row">
        <div class="col-sm-6">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title"><i class="fa fa-question-circle">&nbsp;</i>First party</h3>
                </div>
                <div class="box-body">
                    <table class="table table-striped table-hover">
                        <thead>
                        <tr><th>Package</th><th>Version</th></tr>
                        </thead>
                        <tbody>
                        {% for key,version in selfVersions %}
                            <tr>
                                <td>{{ key }}</td><td class="text-blue">{{ (version|split('@'))[0] }}</td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <div class="col-sm-6">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title"><i class="fa fa-question-circle">&nbsp;</i>Third party</h3>
                </div>
                <div class="box-body">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr><th>Package</th><th>Version</th></tr>
                        </thead>
                        <tbody>
                            {% for key,version in versions %}
                                <tr>
                                    <td>{{ key }}</td><td class="text-blue">{{ (version|split('@'))[0] }}</td>
                                </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
{% endblock %}
